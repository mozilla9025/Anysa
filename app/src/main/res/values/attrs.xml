<?xml version="1.0" encoding="utf-8"?>
<resources>


    <declare-styleable name="RegisterFormInputView">
        <attr name="rfiv_text" format="string" />
        <attr name="rfiv_hint" format="string" />
        <attr name="rfiv_title" format="string" />
        <attr name="rfiv_type" format="enum" >
            <enum name="text" value="0"/>
            <enum name="phone" value="1"/>
            <enum name="password" value="2"/>
        </attr>
        <attr name="rfiv_maxLines" format="integer" />
    </declare-styleable>

</resources>